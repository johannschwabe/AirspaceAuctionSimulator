# editorconfig.org
root = true

[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.py]
indent_size = 4
ij_python_continuation_indent_size = 8
ij_continuation_indent_size = 8

[*.md]
trim_trailing_whitespace = false
